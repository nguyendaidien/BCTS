on>					<xs:documentation>For IPTUPD, IPTUPT (Refund)</xs:documentation>				</xs:annotation>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="refund_onlyType">		<xs:sequence>			<xs:element name="header" type="refundheaderType"/>			<xs:element name="declarant" type="declarantType">				<xs:annotation>					<xs:documentation>dcl</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="support_doc" type="support_docType" minOccurs="0"/>			<xs:element name="refund_item" type="refund_itemType" minOccurs="0" maxOccurs="50"/>			<xs:element name="refund_summary" type="refund_summaryType"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="refundType">		<xs:sequence>			<xs:element name="refund_ref_no" minOccurs="0">				<xs:annotation>					<xs:documentation>Provided by Customs</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:minLength value="1"/>						<xs:maxLength value="13"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="reason_code">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:minLength value="1"/>						<xs:maxLength value="4"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="reason" minOccurs="0" maxOccurs="4">				<xs:annotation>					<xs:documentation>Specify free text reason for refund if reason for refund code =others.</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:minLength value="1"/>						<xs:maxLength value="70"/>					</xs:restriction>				</xs:simpleType>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="refund_app_condType">		<xs:sequence>			<xs:element name="cond_code">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:minLength value="1"/>						<xs:maxLength value="4"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="cond_code_desc">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:minLength value="1"/>						<xs:maxLength value="512"/>					</xs:restriction>				</xs:simpleType>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="permitType">		<xs:sequence>			<xs:element name="pmt_no">				<xs:annotation>					<xs:documentation>Permit No. or Update Permit No.</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:length value="11"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="pmt_app_date_time">				<xs:annotation>					<xs:documentation>Permit Approval Date Time</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:minLength value="1"/>						<xs:maxLength value="17"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="refund_app_cond" type="refund_app_condType" minOccurs="0" maxOccurs="20">				<xs:annotation>					<xs:documentation>Refund Approval Conditions</xs:documentation>				</xs:annotation>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="messageType">		<xs:sequence>			<xs:choice>				<xs:element name="cancellation" type="cancellationType">					<xs:annotation>						<xs:documentation>See Customs Response XSD for Cancellation Approval Message</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="refund_only" type="refund_onlyType"/>			</xs:choice>			<xs:element name="update" type="UpdateType" minOccurs="0">				<xs:annotation>					<xs:documentation>For refund only and cancellation</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="permit" type="permitType" minOccurs="0">				<xs:annotation>					<xs:documentation>Provided by Customs:Approval for Refund Only</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="add_info" type="add_infoType" minOccurs="0" maxOccurs="100">				<xs:annotation>					<xs:documentation>Additional InformationFor Future Use</xs:documentation>				</xs:annotation>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:complexType name="tradenetType">		<xs:sequence>			<xs:element name="msg_ver" fixed="041">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:minLength value="1"/>						<xs:maxLength value="3"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="message" type="messageType"/>		</xs:sequence>		<xs:attribute name="date_time">			<xs:annotation>				<xs:documentation>Format: CCYYMMDDHHSS</xs:documentation>			</xs:annotation>			<xs:simpleType>				<xs:restriction base="xs:decimal">					<xs:totalDigits value="12"/>					<xs:fractionDigits value="0"/>				</xs:restriction>			</xs:simpleType>		</xs:attribute>	</xs:complexType></xs:schema>