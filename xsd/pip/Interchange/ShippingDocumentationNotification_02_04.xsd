<xs:appinfo>
<urss:Definition>This object represents the party where the fund transfers from.</urss:Definition>
</xs:appinfo>
</xs:annotation>
</xs:element>
<xs:element ref="upi:PartnerDescription" minOccurs="0" maxOccurs="unbounded"/>
