<xs:complexType name="AmendmentInformationType">
<xs:annotation>
<xs:appinfo>
<urss:Definition>This object represents a collection of information regarding the amendment needed on an approved customs declaration.</urss:Definition>
<urss:CreationDate>2006-04-19</urss:CreationDate>
<urss:LastUpdatedDate>2006-04-19</urss:LastUpdatedDate>
<urss:TypeVersion>01.00</urss:TypeVersion>
</xs:appinfo>
</xs:annotation>
<xs:sequence>
<xs:element name="Code" type="xs:string">
<xs:annotation>
<xs:appinfo>
<urss:Definition>This object represents the type of changes made.</urss:Definition>
</xs:appinfo>
</xs:annotation>
</xs:element>
<xs:element name="Identifier" type="xs:string">
<xs:annotation>
<xs:appinfo>
<urss:Definition>This object represents a sequencial number to identify the amendment made.</urss:Definition>
</xs:appinfo>
</xs:annotation>
</xs:element>
<xs:element name="PermitNumber" type="xs:string">
<xs:annotation>
<xs:appinfo>
<urss:Definition>This object represents the permit number of the permit that is being amended.</urss:Definition>
</xs:appinfo>
</xs:annotation>
</xs:element>
<xs:element name="Reason" type="xs:string">
<xs:annotation>
<xs:appinfo>
<urss:Definition>This object represents a textual description of the reason for the amendment made.</urss:Definition>
</xs:appinfo>
</xs:annotation>
</xs:element>
<xs:element name="TotalAmendedField" type="xs:string" minOccurs="0">
<xs:annotation>
<xs:appinfo>
<urss:Definition>This object represents the count for the number of fields amended.</urss:Definition>
</xs:appinfo>
</xs:annotation>
</xs:element>
</xs:sequence>
<xs:attribute name="schemaVersion" type="xs:token"/>
</xs:complexType>
<xs:complexType name="DateInformationType">
<xs:annotation>
<xs:appinfo>
<urss:Definition>The collection of business properties that identifies a set of dates associated with this transaction.</urss:Definition>
<urss:CreationDate>2006-04-19</urss:CreationDate>
<urss:LastUpdatedDate>2006-04-19</urss:LastUpdatedDate>
<urss:TypeVersion>01.00</urss:TypeVersion>
</xs:appinfo>
</xs:annotation>
<xs:sequence>
<xs:element name="StartCargoRemovalDate" type="xs:date" minOccurs="0"/>
<xs:element name="EndTemporaryImportDate" type="xs:date" minOccurs="0"/>
<xs:element name="PermitApprovalDate" type="xs:date" minOccurs="0"/>
<xs:element name="OriginalPermitApprovalDate" type="xs:date" minOccurs="0"/>
<xs:element name="PermitValidityDatePeriod" type="ud:StartAndEndDateType" minOccurs="0">
<xs:annotation>
<xs:appinfo>
<urss:Definition>This object represents the start and end date for the validity of the permit.</urss:Definition>
</xs:appinfo>
</xs:annotation>
</xs:element>
<xs:element name="CAApprovalDateTime" type="xs:dateTime" minOccurs="0"/>
<xs:element name="PermitApprovalDateTime" type="xs:dateTime" minOccurs="0"/>
</xs:sequence>
<xs:attribute name="schemaVersion" type="xs:token"/>
</xs:complexType>
<xs:complexType name="DeclarationInformationType">
<xs:annotation>
<xs:appinfo>
<urss:Definition>The collection of business properties that describes the information about import or export declaration of a shipment.</urss:Definition>
<urss:CreationDate>2006-02-02</urss:CreationDate>
<urss:LastUpdatedDate>2006-05-19</urss:LastUpdatedDate>
<urss:TypeVersion>01.01</urss:TypeVersion>
</xs:appinfo>
</xs:annotation>
<xs:sequence>
